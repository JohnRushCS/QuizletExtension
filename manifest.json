{
	"manifest_version" : 2,
	"name" : "Quizlet Extension",
	"version" : "0.1" ,

	"content_scripts" : [
		{
			"matches" : [
					"<all_urls>"
			], 
			"js" : ["content.js"]
		}
	],

	"browser_action" : {
		"default_icon" : "quizlet_logo.png",
		"default_popup" : "popup.html"
	},

	"background" : {
		"scripts" : ["background.js"]
	}
}

